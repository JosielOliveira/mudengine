{
	"containerDefinitions": [
		{
			"name": "mud-message-container", 
			"image": "406396693860.dkr.ecr.us-east-2.amazonaws.com/mud-message:latest", 
			"memoryReservation": 300, 
			"portMappings": [
				{
				  "hostPort": 0,
				  "protocol": "tcp",
				  "containerPort": 8080
				}
			],
			"essential": true, 
			"environment": [
				{
				  "name": "CONSUL_HOST",
				  "value": "http://ec2-52-14-59-146.us-east-2.compute.amazonaws.com"
				},
				{
				  "name": "CONSUL_PORT",
				  "value": "8500"
				},
				{
				  "name": "VAULT_ADDR",
				  "value": "http://ec2-52-14-59-146.us-east-2.compute.amazonaws.com:8200"
				},
				{
				  "name": "VAULT_TOKEN",
				  "value": "393bda7d-c44a-2c33-6d69-c8d47c83094a"
				},
                {
                  "name": "DEPLOY_ENV",
                  "value": "AWS"
                },
				{
				  "name": "SPRING_PROFILES_ACTIVE",
				  "value": "prod"
				}
			  ],
			"logConfiguration": {
				"logDriver": "awslogs",
				"options": {
				  "awslogs-group": "mudengine",
				  "awslogs-region": "us-east-2",
				  "awslogs-stream-prefix": "mud-message"
				}
			}
		}
	], 
	"family": "mud-message-task-def", 
	"requiresCompatibilities": [
		"EC2"
	]
}

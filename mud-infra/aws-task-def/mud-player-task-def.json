{
	"containerDefinitions": [
		{
			"name": "mud-player-container", 
			"image": "406396693860.dkr.ecr.us-east-2.amazonaws.com/mud-player:latest", 
			"memoryReservation": 300, 
			"portMappings": [
				{
				  "hostPort": 0,
				  "protocol": "tcp",
				  "containerPort": 8080
				}
			],
			"essential": true, 
			"environment": [
				{
				  "name": "CONFIG_SERVER_URL",
				  "value": "http://ec2-52-14-59-146.us-east-2.compute.amazonaws.com:8888"
				},
				{
				  "name": "SPRING_PROFILES_ACTIVE",
				  "value": "prod"
                },
                {
                  "name": "DEPLOY_ENV",
                  "value": "AWS"
				}
			  ],
			"logConfiguration": {
				"logDriver": "awslogs",
				"options": {
				  "awslogs-group": "mudengine",
				  "awslogs-region": "us-east-2",
				  "awslogs-stream-prefix": "mud-player"
				}
			}
		}
	], 
	"family": "mud-player-task-def", 
	"requiresCompatibilities": [
		"EC2"
	]
}

			"logged": false,
			"description": "Creates a account in the system.",
			"usage": "register [<username> [<email> [<locale>] ] ]",
			"parameters": [
				{
					"name": "username",
					"inputMessage": "Please enter the username",
					"type": "anyString",
					"required": true
				},
				{
					"name": "email",
					"inputMessage": "Please enter the email",
					"type": "email",
					"required": true
				},
				{
					"name": "locale",
					"inputMessage": "Please choose a locale",
					"type": "anyString",
					"required": false,
					"domainValues": [
						"en_US",
						"pt_BR"
					],
					"defaultValue": "en_US"
				}
			]
		},
		{
			"verb": "change profile",
			"category": "SYSTEM",
			"logged": true,			
			"description": "Changes profile details. (Must be logged in order to use)",

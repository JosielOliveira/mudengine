			"parameters": [
				{
					"name": "activationCode",
					"inputMessage": "Please type the activation code received",
					"type": "anyString",
					"required": true
				},
				{
					"name": "newPassword",
					"inputMessage": "Please type the new password",
					"type": "anyString",
					"required": true
				}
